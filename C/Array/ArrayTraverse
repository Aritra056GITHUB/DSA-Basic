#include <stdio.h>
#include <stdlib.h>

int main(){
    int n;
    scanf("%d", &n);
    int* arr;
    arr = (int *)malloc(sizeof(int)*n);
    for(int i = 0; i<n; i++){
        scanf("%d", &arr[i]);
    }
    printf("Traversing through the array and printing it\n");
    for(int i = 0; i<n; i++){
        printf("%d ", arr[i]);
    }
}
